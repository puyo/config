#!/bin/bash

gensh() {
  ps ax -o pid,comm | grep -i -E 'gensh|wine|proton|HYPHelper|\.exe' | grep -v -E 'killgensh|grep'
}

before=$(gensh)

if test "$before" = ""; then
  echo "No gensh found"
  exit 0
fi

echo "BEFORE"
echo "${before}"

gensh | ruby -n -e 'puts $_.split.first' | xargs --verbose kill
sleep 2
killall --verbose -9 winedevice.exe

echo
echo "AFTER"
gensh
