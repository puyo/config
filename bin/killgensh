#!/bin/bash

gensh() {
  ps ax -o pid,comm | grep -i -E 'gensh|wine|proton|HYPHelper|\.exe' | grep -v -E 'killgensh|grep'
}

before=$(gensh)

if test "$before" = ""; then
  echo "No gensh found"
  exit 0
fi

echo "BEFORE"
echo "${before}"

gensh | cut -d ' ' -f 1 | xargs kill
sleep 2
killall -9 winedevice.exe

echo
echo "AFTER"
gensh
